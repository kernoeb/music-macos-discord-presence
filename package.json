{
  "name": "music-discord-presence",
  "description": "Display music playback from Telegram and YouTube Music as Discord Rich Presence",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun --watch index.ts",
    "test": "bun run index.ts --test",
    "tray": "bun run index.ts --tray",
    "build": "bun build --compile --minify --define IS_COMPILED=true index.ts --outfile music-discord-presence",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/discord-rpc": "^4.0.10"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.3.0",
    "discord-rpc": "^4.0.1",
    "systray2": "^2.1.4"
  },
  "trustedDependencies": [
    "register-scheme"
  ]
}
